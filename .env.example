# Weather.com API key
WU_API_KEY=

# Semicolon-separated list of geocodes, e.g. "51.50999832,-0.13;40.71,-74.01"
WU_GEOCODES=

# Poll interval in minutes (default 10)
POLL_MINUTES=10

# Output directory (default ./data)
OUTPUT_DIR=./data

# Target location id (default 51.50999832,-0.13)
TARGET_LOCATION_ID=51.50999832,-0.13

# Timezone for local date grouping (default Europe/London)
TIMEZONE=Europe/London
